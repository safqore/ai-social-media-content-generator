{
    "name": "ai-smcg-vscode",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "ai-smcg",
    "workspaceFolder": "/ai-smcg",
    "customizations": {
      "vscode": {
        "extensions": [
            "ms-python.python",
            "ms-vscode-remote.remote-containers",
            "ms-azuretools.vscode-docker",
            "GitHub.copilot"
        ]
      },
      "settings": {
        "python.pythonPath": "/opt/conda/envs/ai-smcg-env/bin/python"
      }
    }
}